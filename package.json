{
  "name": "eslint-config-7geese",
  "version": "0.0.6",
  "description": "An eslint config to enforce JavaScript coding style at 7Geese.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --config .eslintrc *.js",
    "find-rules": "eslint-find-rules --unused",
    "print-config": "eslint --print-config index.js > new-config.json",
    "prepublish": "yarn outdated && yarn run find-rules && yarn run lint"
  },
  "engines": {
    "node": ">= 6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "5.0.1",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-7g": "1.0.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0"
  },
  "peerDependencies": {
    "babel-eslint": ">= 8.2.0",
    "eslint": ">= 5.0.0",
    "eslint-plugin-7g": ">= 1.0.0",
    "eslint-plugin-compat": ">= 2.4.0",
    "eslint-plugin-filenames": ">= 1.3.0",
    "eslint-plugin-import": ">= 2.13.0",
    "eslint-plugin-jest": ">= 21.17.0",
    "eslint-plugin-promise": ">= 3.8.0",
    "eslint-plugin-react": ">= 7.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7Geese/eslint-config-7geese.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "lint",
    "7geese",
    "javascript",
    "linter-config",
    "linting-rules",
    "eslintconfig",
    "code checker",
    "code linter",
    "code standards",
    "code style"
  ],
  "author": "7Geese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/7Geese/eslint-config-7geese/issues"
  },
  "homepage": "https://github.com/7Geese/eslint-config-7geese#readme",
  "private": false,
  "directories": {
    "doc": "docs"
  }
}
