{
  "name": "eslint-config-7geese",
  "version": "4.0.0",
  "description": "An eslint config to enforce JavaScript coding style at 7Geese.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --config .eslintrc *.js",
    "find-rules": "eslint-find-rules --unused",
    "print-config": "eslint --print-config index.js > config-full.json",
    "test": "npm outdated && npm run find-rules && npm run lint",
    "release": "np --no-yarn"
  },
  "engines": {
    "node": ">= 6"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.7.1",
    "eslint-find-rules": "3.4.0",
    "eslint-plugin-7g": "3.6.0",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "np": "4.0.2"
  },
  "peerDependencies": {
    "eslint": "6.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7Geese/eslint-config-7geese.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "lint",
    "7geese",
    "javascript",
    "linter-config",
    "linting-rules",
    "eslintconfig",
    "code checker",
    "code linter",
    "code standards",
    "code style"
  ],
  "author": "7Geese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/7Geese/eslint-config-7geese/issues"
  },
  "homepage": "https://github.com/7Geese/eslint-config-7geese#readme",
  "private": false,
  "directories": {
    "doc": "docs"
  }
}
