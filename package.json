{
  "name": "eslint-config-7geese",
  "version": "2.1.0",
  "description": "An eslint config to enforce JavaScript coding style at 7Geese.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --config .eslintrc *.js",
    "find-rules": "eslint-find-rules --unused",
    "print-config": "eslint --print-config index.js > config-full.json",
    "test": "npm outdated && npm run find-rules && npm run lint",
    "release": "np --no-yarn"
  },
  "engines": {
    "node": ">= 6"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-find-rules": "3.3.1",
    "eslint-plugin-7g": "3.0.0",
    "eslint-plugin-compat": "2.7.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.3.0",
    "np": "4.0.2"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-plugin-7g": "^3.0.0",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7Geese/eslint-config-7geese.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "lint",
    "7geese",
    "javascript",
    "linter-config",
    "linting-rules",
    "eslintconfig",
    "code checker",
    "code linter",
    "code standards",
    "code style"
  ],
  "author": "7Geese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/7Geese/eslint-config-7geese/issues"
  },
  "homepage": "https://github.com/7Geese/eslint-config-7geese#readme",
  "private": false,
  "directories": {
    "doc": "docs"
  }
}
